# =============================================================================
# OpusVoice — Environment Variables Template
# Copy to .env and fill in real values
# =============================================================================

# --- AWS Bedrock (PRIMARY — hackathon temp bearer token, ~12h expiry) ---
AWS_BEARER_TOKEN_BEDROCK=bedrock-api-key-YOUR_BEARER_TOKEN_HERE
AWS_DEFAULT_REGION=us-west-2

# --- AWS Bedrock (BACKUP — personal ABSK key) ---
AWS_BEDROCK_API_KEY_BACKUP=ABSK_YOUR_BACKUP_KEY_HERE

# --- MiniMax (get free credits: https://forms.gle/Fazk8r87QmudNLNd6) ---
MINIMAX_API_KEY=your_minimax_api_key_here

# --- Datadog (https://app.datadoghq.com → Org Settings → API Keys) ---
DD_API_KEY=your_datadog_api_key_here
DD_APP_KEY=your_datadog_app_key_here
DD_SITE=datadoghq.com
DD_LLMOBS_ENABLED=1
DD_LLMOBS_ML_APP=opusvoice
DD_LLMOBS_AGENTLESS_ENABLED=true
DD_SERVICE=opusvoice-backend
DD_ENV=hackathon

# --- PostgreSQL ---
POSTGRES_USER=opusvoice
POSTGRES_PASSWORD=opusvoice_hack2026
POSTGRES_DB=opusvoice
DATABASE_URL=postgresql://opusvoice:opusvoice_hack2026@postgres:5432/opusvoice
